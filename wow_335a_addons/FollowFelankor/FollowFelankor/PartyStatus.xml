<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Script file="FFEvents.lua"/>
<!-- Start FF_PartyMember1 Frame -->
<Frame name="FF_PartyMember1" parent="PartyMemberFrame1" hidden="true" toplevel="true">
   <Size>
      <AbsDimension x="28" y="28"/>
   </Size>
   <Anchors>
      <Anchor point="TOPLEFT" relativeTo="PartyMemberFrame1" relativePoint="TOPLEFT">
         <Offset>
            <AbsDimension x="15" y="0" />
         </Offset>
      </Anchor>
   </Anchors>
   <Frames>
        <Button name="$parentStatus">
            <Size>
                <AbsDimension x="28" y="28"/>
            </Size>
            <Anchors>
                <Anchor point="TOPLEFT">
                    <Offset>
                        <AbsDimension x="0" y="0"/>
                    </Offset>
                </Anchor>
            </Anchors>
            <Layers>
                <Layer level="BORDER">
                    <Texture name="$parentIcon" file="Interface\AddOns\FollowFelankor\Images\FF_MinimapEnabled">
                        <Size>
                            <AbsDimension x="9" y="9"/>
                        </Size>
                        <Anchors>
                            <Anchor point="TOPLEFT">
                                <Offset>
                                    <AbsDimension x="4" y="-4"/>
                                </Offset>
                            </Anchor>
                        </Anchors>
                    </Texture>
                </Layer>
                <Layer level="OVERLAY">
                    <Texture name="$parentBorder" file="Interface\Minimap\MiniMap-TrackingBorder">
                        <Size>
                            <AbsDimension x="28" y="28"/>
                        </Size>
                        <Anchors>
                            <Anchor point="TOPLEFT"/>
                        </Anchors>
                    </Texture>
                </Layer>
            </Layers>
        </Button>
    </Frames>
</Frame>
<!-- End FF_PartyMember1 Frame -->
